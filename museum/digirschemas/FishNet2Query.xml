<request 
    xmlns="http://digir.net/schema/protocol/2003/1.0" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
    xmlns:darwin="http://digir.net/schema/conceptual/darwin/2003/1.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://digir.net/schema/conceptual/darwin/2003/1.0 http://bnhm.berkeley.museum/manis/DwC/darwin2jrw030315.xsd">
	<header>
            <version>1.0</version>
            <sendTime/>
            <source/>
            <destination resource="FishDwC2"/>
            <type>search</type>
	</header>
	<search>
            <filter>
                <like>
                    <darwin:Locality>%</darwin:Locality>
                </like>
            </filter>
            <records limit="1000" start="998">
                <structure schemaLocation="http://digir.sourceforge.net/schema/conceptual/darwin/full/2003/1.0/darwin2full.xsd"/>
            </records>
            <count>true</count>
	</search>
</request>